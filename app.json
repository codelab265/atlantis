      "package": "com.atlantis.codelab",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCG9E5E7_YCna0L7GCr8BJgGjLlxkkJE-8"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
